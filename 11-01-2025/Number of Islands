def numIslands(g):
    def dfs(i, j):
        if i<0 or j<0 or i>=len(g) or j>=len(g[0]) or g[i][j]=='0':
            return
        g[i][j]='0'
        dfs(i+1,j); dfs(i-1,j); dfs(i,j+1); dfs(i,j-1)
    c = 0
    for i in range(len(g)):
        for j in range(len(g[0])):
            if g[i][j]=='1':
                dfs(i,j)
                c+=1
    return c
