def minWindow(s, t):
    from collections import Counter
    c = Counter(t)
    l = r = f = 0
    res = ""
    for r in range(len(s)):
        if s[r] in c:
            if c[s[r]] > 0:
                f += 1
            c[s[r]] -= 1
        while f == len(t):
            if not res or r-l+1 < len(res):
                res = s[l:r+1]
            if s[l] in c:
                c[s[l]] += 1
                if c[s[l]] > 0:
                    f -= 1
            l += 1
    return res
