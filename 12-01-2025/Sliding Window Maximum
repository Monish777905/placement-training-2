from collections import deque
def maxSlidingWindow(nums, k):
    d, res = deque(), []
    for i in range(len(nums)):
        while d and nums[d[-1]] < nums[i]:
            d.pop()
        d.append(i)
        if d[0] == i - k:
            d.popleft()
        if i >= k - 1:
            res.append(nums[d[0]])
    return res
